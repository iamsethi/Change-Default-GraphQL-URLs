type Query {
	orgId : Int
	orgName : String
	employee(id : Long!) : employeeResponse
}

type employeeResponse {
	id : Long
	firstName : String
	lastName : String
	email : String
	teamName(teamNameFilter : teamNameFilter) : [Team]
}

type Team {
	teamId : Long
	teamName : String
}

enum teamNameFilter {
	R360,
	Telemetry
}